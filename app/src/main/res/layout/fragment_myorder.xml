<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="263dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="44dp"
        android:text="Myorder page"
        android:textColor="@color/purple_200"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/upload_prescripbtn"
        android:layout_width="319dp"
        android:layout_height="85dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="@android:color/transparent"
        android:drawableLeft="@drawable/ic_vitamins_suppliment"
        android:text="Uploaded Precription"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.412"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/myorderbtn"
        android:layout_width="313dp"
        android:layout_height="69dp"
        android:layout_marginTop="56dp"
        android:background="@android:color/transparent"
        android:drawableLeft="@drawable/ic_shopping_bag"
        android:text="Shopping Orders"
        android:textColor="#191818"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.387"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/upload_prescripbtn" />

</androidx.constraintlayout.widget.ConstraintLayout>